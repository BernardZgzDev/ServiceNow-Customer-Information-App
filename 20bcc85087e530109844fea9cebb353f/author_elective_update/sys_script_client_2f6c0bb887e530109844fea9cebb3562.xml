        <applies_extended>false</applies_extended>
        <condition/>
        <description>Hide AMS Section is 'Current scope' field IS NOT Application Maintenance Service (AMS)</description>
        <field>ams_hours</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Display AMS Section onLoad</name>
        <order/>
        <script><![CDATA[function onLoad() {
	
	try {
		
		var currentScope =  g_form.getValue('current_scope');
		var isShown = false;

		if ( currentScope === 'ams') {

			isShown = true;
		}
		g_form.setSectionDisplay('ams_details', isShown);
	
	} catch ( err ) {
		
		g_form.addErrorMessage('Runtime error: ' + err );
	}
}]]></script>
