        <messages/>
        <name>Dates validation onSubmit</name>
        <order/>
        <script><![CDATA[function onSubmit() {
   
	try {
		
		var validFrom = g_form.getValue('valid_from');
		var validTo = g_form.getValue('valid_to');
		
		jslog('BAMN LOGS: ' + validFrom);
		jslog('BAMN LOGS: ' + validTo);
		
		if( validFrom && validTo ) {
			
			var validateDates = new StartEndDateValidation();
			
			var isValid = validateDates.validateStartDateBeforeEndDate(validFrom, validTo, true);
			
			g_form.addInfoMessage( isValid );			
		}
		
		
		
	} catch ( err ) {
		
		g_form.addErrorMessage('Runtime error: ' + err.name + ', ' + err.message);
		return false;
	}    
}]]></script>
